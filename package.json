{
  "name": "uptown",
  "version": "1.0.0",
  "description": "Simple ways to extend prototypes",
  "main": "lib/uptown.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:full && npm run build:min",
    "build:full": "browserify -d -s uptown -x lodash -o dist/uptown.js lib/uptown.js",
    "build:min": "browserify -d -s uptown lib/uptown.js | uglifyjs > dist/uptown.min.js",
    "test": "mocha --recursive"
  },
  "keywords": [
    "ES6",
    "class",
    "extend",
    "getters",
    "setters",
    "inherit"
  ],
  "author": "Stephen Mizell",
  "homepage": "https://github.com/smizell/uptown",
  "license": "MIT",
  "files": [
    "dist/uptown.js",
    "dist/uptown.min.js"
  ],
  "devDependencies": {
    "browserify": "^14.5.0",
    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "uglify-js": "^3.2.2"
  },
  "dependencies": {
    "lodash": "^4.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smizell/uptown.git"
  }
}
